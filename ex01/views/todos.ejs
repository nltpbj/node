<h1><%=title%></h1>
<div id="todos">데이터없음</div>
<!--할일목록 템플릿-->
<script id="temp" type="x-handlebars-template">
    <table class="table table-striped table-hover">
        {{#each .}}                                                                 
            <tr>
                <td>{{comlpeted}}</td>
                <td>{{id}}</td>
                <td>{{title}}</td>
            </tr>
        {{/each}}
    </table>
</script>                                                             
<script>
    getTodos(); //함수호출

    function getTodos(){                                                             //함수정의 Todos라는 함수는 밑에사이트에서 데이터를 리스폰스에 저장하고 제이손으로 바꿔준다
        fetch('https://jsonplaceholder.typicode.com/todos')                         // https://jsonplaceholder.typicode.com/ 에서 가져옴 //
        .then(response => response.json())
        .then(json => {
            console.log(json);                                                       //여기는 그 제이손을 콘솔로그에 넣은것
            const temp=Handlebars.compile($("#temp").html());
            $("#todos").html(temp(json))                                
        })
    }   
</script>